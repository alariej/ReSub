{
  "name": "resub",
  "version": "2.3.0",
  "description": "A library for writing React components that automatically manage subscriptions to data sources simply by accessing them.",
  "author": "ReactXP Team <reactxp@microsoft.com>",
  "scripts": {
    "prepare": "npm run build",
    "clean:dist": "rimraf dist*",
    "build:types": "tsc -p tsconfig/types.json",
    "build:es2015": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig/cjs.json",
    "build": "run-s clean:* lint build:*",
    "lint": "eslint --config .eslintrc --ext .ts,.tsx src",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "tslib": "1.11.1",
    "tsutils": "2.0.0"
  },
  "peerDependencies": {
    "react": "17.0.2"
  },
  "devDependencies": {
    "@types/jasmine": "3.5.10",
    "@types/lodash": "4.14.149",
    "@types/react": "16.9.29",
    "@types/react-dom": "16.9.5",
    "@typescript-eslint/eslint-plugin": "2.26.0",
    "@typescript-eslint/parser": "2.26.0",
    "eslint": "6.8.0",
    "eslint-config-skype": "1.6.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-react": "7.19.0",
    "fork-ts-checker-webpack-plugin": "4.1.2",
    "jasmine": "3.5.0",
    "jasmine-core": "3.5.0",
    "lodash": "4.17.15",
    "npm-run-all": "4.1.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "3.0.2",
    "ts-loader": "6.2.2",
    "tslint": "6.1.0",
    "typescript": "3.8.3",
    "webpack": "4.42.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/ReSub"
  },
  "bugs": {
    "url": "https://github.com/Microsoft/ReSub/issues"
  },
  "types": "dist-types/ReSub.d.ts",
  "module": "dist-es2015/ReSub.js",
  "main": "dist/ReSub.js",
  "keywords": [
    "react",
    "flux",
    "automatic",
    "subscriptions",
    "stores"
  ],
  "license": "MIT"
}
